{
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "experimentalDecorators": true,
    "importHelpers": false,
    "noEmit": true,
    "removeComments": true,
    "module": "commonjs",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "allowJs": true
  },
  "exclude": [
    "dist"
  ]
}